<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.molars.molars">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.DashboardActivity" />
        <activity
            android:name=".activities.AboutUsActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activities.DashboardActivity" />
        <activity
            android:name=".activities.ServicesActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activities.DashboardActivity" />
        <activity
            android:name=".activities.SuccessStoriesActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activities.DashboardActivity" />
        <activity
            android:name=".activities.VideosActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activities.DashboardActivity" />
        <activity
            android:name=".activities.ContactUsActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activities.DashboardActivity" />
        <activity
            android:name=".activities.LocationActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activities.DashboardActivity" />
        <activity
            android:name=".activities.TeamActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activities.DashboardActivity" />
        <activity
            android:name=".activities.BookAppointmentActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activities.DashboardActivity" />
    </application>

</manifest>